{
    "manifest_version": 2,
    "name": "Invitation Block POC",
    "description": "This chrome extension inject/clean RingCentral meeting/conference invitation block on office 365 calendar",
    "version": "1.0",
    "icons": {
        "128": "rc128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://outlook.office.com/*"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}